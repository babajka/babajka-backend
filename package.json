{
  "name": "babajka-backend",
  "version": "0.0.1",
  "description": "Express & Node.js app",
  "scripts": {
    "start": "nodemon -w index.js -w . --exec babel-node index.js",
    "start-prod": "npm run build && node build/index.js",
    "build": "babel . -d build --copy-files --ignore node_modules,postman,build",
    "init-db": "babel-node src/db/init.js",
    "lint": "eslint index.js src/",
    "mocha": "find ./src -name '*tests.js' | xargs mocha --require babel-register",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test": "nyc npm run mocha && npm run coverage"
  },
  "engines": {
    "node": ">=4.3.2"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "connect-mongo": "^1.3.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.3",
    "express": "^4.15.3",
    "express-session": "^1.15.4",
    "lodash": "^4.17.4",
    "mongoose": "^4.11.7",
    "morgan": "^1.8.2",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/babajka/babajka-backend.git"
  },
  "author": "babajka team <github.com/babajka>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/babajka/babajka-backend/issues"
  },
  "homepage": "https://github.com/babajka/babajka-backend#readme",
  "main": "index.js",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.1",
    "coveralls": "^2.13.1",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.7.0",
    "mocha": "^3.5.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.11.0",
    "nyc": "^11.1.0",
    "supertest": "^3.0.0"
  }
}
