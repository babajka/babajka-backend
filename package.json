{
  "name": "babajka-backend",
  "version": "0.0.1",
  "description": "Express & Node.js app",
  "scripts": {
    "start": "nodemon -w index.js -w . --exec babel-node index.js",
    "start-prod": "npm run build && node build/index.js",
    "build": "babel . -d build --copy-files --ignore node_modules,postman,build",
    "init-db": "babel-node src/db/init.js",
    "lint": "eslint index.js src/",
    "test": "mocha --reporter spec src/**/*.test.js --require babel-register"
  },
  "engines": {
    "node": ">=4.3.2"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "body-parser": "^1.17.2",
    "chai": "^4.1.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.3",
    "express": "^4.15.3",
    "mocha": "^3.4.2",
    "mongoose": "^4.10.8",
    "morgan": "^1.8.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/babajka/babajka-backend.git"
  },
  "author": "babajka team <github.com/babajka>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/babajka/babajka-backend/issues"
  },
  "homepage": "https://github.com/babajka/babajka-backend#readme",
  "main": "index.js",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-regenerator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "eslint": "^4.1.1",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-config-airbnb-base": "^11.2.0",
    "nodemon": "^1.11.0"
  }
}
